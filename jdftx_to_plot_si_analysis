using jdftx_to_plot

print("hello")

filenames = []
#plot_bands("bandstruct.eigenvals", 40, 49)
for filename in readdir()
    if filename[end-5:end] == "ations"
        l = length(filename[11:end])
        parse(Int, filename[11:end][1:l-12])
        numfilename = (parse(Int, filename[11:end][1:l-12]), filename)
        push!(filenames, numfilename)
    end
end

onlyfilenames = []

for files in filenames 
    push!(onlyfilenames, files[2])
end

sort!(filenames, by = x -> x[1])



